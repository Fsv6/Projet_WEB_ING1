-form .checkbox-group {\r\n  display: flex;\r\n  gap: 1.5rem;\r\n  margin-bottom: 1.2rem;\r\n  margin-top: 0.5rem;\r\n}\r\n\r\n.edit-object-form .checkbox-group label {\r\n  font-weight: 500;\r\n  color: #036672;\r\n  font-size: 1rem;\r\n}\r\n\r\n.edit-object-form button {\r\n  width: 100%;\r\n  background: linear-gradient(90deg, #22c55e 0%, #036672 100%);\r\n  color: #fff;\r\n  font-weight: bold;\r\n  border: none;\r\n  border-radius: 12px;\r\n  padding: 1.1rem 0;\r\n  font-size: 1.18rem;\r\n  cursor: pointer;\r\n  transition: background 0.2s, box-shadow 0.2s, transform 0.1s;\r\n  margin-top: 1.5rem;\r\n  box-shadow: 0 2px 12px rgba(34,197,94,0.13);\r\n  letter-spacing: 0.5px;\r\n}\r\n\r\n.edit-object-form button:hover {\r\n  background: linear-gradient(90deg, #036672 0%, #22c55e 100%);\r\n  box-shadow: 0 4px 18px rgba(34,197,94,0.22);\r\n  transform: translateY(-2px) scale(1.03);\r\n}\r\n\r\n/* Responsive */\r\n@media (max-width: 600px) {\r\n  .edit-object-container {\r\n    max-width: 98vw;\r\n    padding: 1.2rem 0.5rem;\r\n  }\r\n  .edit-object-title {\r\n    font-size: 1.3rem;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;;;AAoEA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,SAASC,QAAQ,EAAEC,SAAS,QAAQ,YAAY;AAChD,OAAOC,GAAG,MAAM,gBAAgB;AAChC,OAAOC,SAAS,MAAM,8BAA8B;;;;;;;IAEpD,MAAMC,KAAK,GAAGJ,QAAQ,CAAC,CAAC;IACxB,MAAMK,MAAM,GAAGJ,SAAS,CAAC,CAAC;IAC1B,MAAMK,MAAM,GAAGR,GAAG,CAAC,IAAI,CAAC;IACxB,MAAMS,UAAU,GAAGT,GAAG,CAAC,EAAE,CAAC;IAE1B,MAAMU,IAAI,GAAGV,GAAG,CAAC,CAAC,CAAC,CAAC;IAEpBC,SAAS,CAAC,YAAY;MACpB,MAAMU,EAAE,GAAGL,KAAK,CAACM,MAAM,CAACD,EAAE;MAC1B,IAAI;QACF,MAAME,GAAG,GAAG,MAAMT,GAAG,CAACU,GAAG,CAAC,WAAWH,EAAE,EAAE,CAAC;QAC1CH,MAAM,CAACO,KAAK,GAAGF,GAAG,CAACG,IAAI;QACvBN,IAAI,CAACK,KAAK,GAAG;UACXE,GAAG,EAAEJ,GAAG,CAACG,IAAI,CAACC,GAAG;UACjBC,WAAW,EAAEL,GAAG,CAACG,IAAI,CAACE,WAAW;UACjCC,IAAI,EAAEN,GAAG,CAACG,IAAI,CAACG,IAAI;UACnBC,MAAM,EAAEP,GAAG,CAACG,IAAI,CAACI,MAAM;UACvBC,YAAY,EAAER,GAAG,CAACG,IAAI,CAACK,YAAY;UACnCC,MAAM,EAAET,GAAG,CAACG,IAAI,CAACM,MAAM;UACvBC,YAAY
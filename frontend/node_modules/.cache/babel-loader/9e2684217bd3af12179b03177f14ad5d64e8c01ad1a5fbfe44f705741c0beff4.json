{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport { useRoute } from 'vue-router';\nimport AppLayout from '@/layout/AppLayoutGlobal.vue';\nimport api from '../services/api';\nexport default {\n  __name: 'ObjectDetails',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const route = useRoute();\n    const object = ref(null);\n    onMounted(async () => {\n      try {\n        const res = await api.get(`/objets/${route.params.id}`);\n        object.value = res.data;\n      } catch (error) {\n        console.error('Erreur de chargement de l\\'objet :', error);\n      }\n    });\n    const __returned__ = {\n      route,\n      object,\n      ref,\n      onMounted,\n      get useRoute() {\n        return useRoute;\n      },\n      AppLayout,\n      get api() {\n        return api;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","useRoute","AppLayout","api","route","object","res","get","params","id","value","data","error","console"],"sources":["C:/Users/benab/Desktop/COURS 24_25/S2/WEB/V4/Projet_WEB_ING1/frontend/src/views/ObjectDetails.vue"],"sourcesContent":["<script setup>\r\nimport { ref, onMounted } from 'vue'\r\nimport { useRoute } from 'vue-router'\r\nimport AppLayout from '@/layout/AppLayoutGlobal.vue'\r\nimport api from '../services/api'\r\n\r\n\r\nconst route = useRoute()\r\nconst object = ref(null)\r\n\r\nonMounted(async () => {\r\n  try {\r\n    const res = await api.get(`/objets/${route.params.id}`)\r\n    object.value = res.data\r\n  } catch (error) {\r\n    console.error('Erreur de chargement de l\\'objet :', error)\r\n  }\r\n})\r\n</script>\r\n\r\n<template>\r\n  <AppLayout>\r\n    <div class=\"object-details\">\r\n      <router-link to=\"/explore\">\r\n        <button class=\"btn-retour\">← Retour au dashboard !</button>\r\n      </router-link>\r\n\r\n      <h2>Détails de l'objet connecté</h2>\r\n\r\n      <div v-if=\"object\" class=\"object-info\">\r\n        <p><strong>Nom :</strong> {{ object.nom }}</p>\r\n        <p><strong>Description :</strong> {{ object.description }}</p>\r\n        <p><strong>Type :</strong> {{ object.type }}</p>\r\n        <p><strong>Marque :</strong> {{ object.marque }}</p>\r\n        <p><strong>État :</strong> {{ object.statut }}</p>\r\n        <p><strong>Connectivité :</strong> {{ object.connectivite }}</p>\r\n        <p><strong>Batterie :</strong> {{ object.batterie ?? 'N/A' }}%</p>\r\n        <p><strong>Température actuelle :</strong> {{ object.temperatureActuelle }}°C</p>\r\n        <p><strong>Température cible :</strong> {{ object.temperatureCible }}°C</p>\r\n        <p><strong>Mode :</strong> {{ object.mode }}</p>\r\n        <p><strong>Pièce :</strong> {{ object.piece }}</p>\r\n        <p><strong>Dernière interaction :</strong> {{ new Date(object.derniereInteraction).toLocaleString() }}</p>\r\n      </div>\r\n\r\n      <div v-else>\r\n        <p>Chargement...</p>\r\n      </div>\r\n    </div>\r\n  </AppLayout>\r\n</template>\r\n\r\n<style scoped>\r\n.object-details {\r\n  padding: 2rem;\r\n}\r\n.object-info {\r\n  background: #f9f9f9;\r\n  padding: 1rem;\r\n  border-radius: 8px;\r\n}\r\n.btn-retour {\r\n  background-color: #2c3e50;\r\n  color: white;\r\n  padding: 0.5rem 1rem;\r\n  border: none;\r\n  border-radius: 4px;\r\n  margin-bottom: 1rem;\r\n  cursor: pointer;\r\n}\r\n\r\n</style>\r\n"],"mappings":"AACA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,SAASC,QAAQ,QAAQ,YAAY;AACrC,OAAOC,SAAS,MAAM,8BAA8B;AACpD,OAAOC,GAAG,MAAM,iBAAiB;;;;;;;IAGjC,MAAMC,KAAK,GAAGH,QAAQ,CAAC,CAAC;IACxB,MAAMI,MAAM,GAAGN,GAAG,CAAC,IAAI,CAAC;IAExBC,SAAS,CAAC,YAAY;MACpB,IAAI;QACF,MAAMM,GAAG,GAAG,MAAMH,GAAG,CAACI,GAAG,CAAC,WAAWH,KAAK,CAACI,MAAM,CAACC,EAAE,EAAE,CAAC;QACvDJ,MAAM,CAACK,KAAK,GAAGJ,GAAG,CAACK,IAAI;MACzB,CAAC,CAAC,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,oCAAoC,EAAEA,KAAK,CAAC;MAC5D;IACF,CAAC,CAAC","ign